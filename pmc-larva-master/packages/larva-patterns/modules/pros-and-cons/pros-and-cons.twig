<div class="pros-and-cons {{pros_and_cons_classes}}">
	{% for item in items %}
		<div class="pros-and-cons__inner {{pros_and_cons_inner_classes}}" >
			{% if item['c_heading'] %}
				{% include "@larva/components/c-heading/c-heading.twig" with item['c_heading'] %}
			{% endif %}
			{% if item['list_markup'] %}
				{{item['list_markup']|raw}}
			{% endif %}
		</div>
	{% endfor %}
</div>
